package javaBasic2.ch05.sec03;

public class 이차원배열 {

	public static void main(String[] args) {
		
		
		int[][] intArr = { {1,2},{2,3},{3,4} };
		
		String[][] strArr = { {"a","b","c"},{"d","e","f"} };
		
		boolean[][] bArr = { {false,true},{false,true},{true,false},{true,true} };
		
		// 2차원 배열의 값을 출력하기
		System.out.println(intArr[2][1]);
		
		System.out.println(strArr[1][1]);
		
		System.out.println(bArr[3][0]);
		
		// 배열 안에 모든 값 출력하기
		for(int i=0;i<intArr.length;i++) { //i=0, 1, 2
			for(int j=0;j<intArr[i].length;j++) { //j=0, 1
				System.out.print(intArr[i][j] + " "); //옆으로 출력하기
			}
			System.out.println(); //줄바꾸기
		}
	}

}
