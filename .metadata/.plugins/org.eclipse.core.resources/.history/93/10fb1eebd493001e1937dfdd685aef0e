package javaBasic.ch04.sec01;

public class while문연습 {

	public static void main(String[] args) {
		// 2 4 6 8 10 출력해보기
		// 2를 정수형 변수에 넣기
		// 정수형 변수를 2씩 더한다.
		// 반복해서 출력한다.
		// 10이 넘으면 종료한다.
		
		int number=2;
		while (number<10) {
			number= number+2;
			if (number>10) { break;
			} else {
				System.out.println(number);
			}
			
					
		}
				

	}

}
